summary: Execute all Playwright E2E tests.
tag:
  - all
duration: 2h
result: custom
framework: shell
test: |
  set +e
  git clone -b $BRANCH https://github.com/$FORK/podman-desktop.git /$TMT_TREE/podman-desktop
  cd $TMT_TREE/podman-desktop
  pnpm install
  pnpm test:e2e:all
  EXIT_CODE=$?
  bash "$TMT_TREE/tmt/create-results.sh" "$EXIT_CODE" "all"
